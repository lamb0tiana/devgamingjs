{
    "compilerOptions": {
      "sourceMap": true,
      "target": "es5",
      "allowSyntheticDefaultImports": true,
      "noEmitOnError": true,
      "allowJs": true,
      "noImplicitAny": false,
      "outDir": "./dist",
      "baseUrl": "."
    },
    "compileOnSave": true,
    "exclude": [
      "node_modules"
    ],
    "include": ["./src/**/*","./dist/**/*"]
  }